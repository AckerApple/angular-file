    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "0.4.0",
    "karma-sauce-launcher": "1.1.0",
    "karma-typescript": "^3.0.5",
    "ngx-bootstrap": "^1.9.1",
    "npm-run-all": "^4.1.1",
    "phantomjs-prebuilt": "^2.1.15",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^2.3.4",
    "ts-node": "2.0.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.5",
    "zone.js": "^0.8.17",
