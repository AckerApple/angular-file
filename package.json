    "@angular/platform-browser-dynamic": "^5.0.5",
    "@types/jasmine": "^2.8.2",
    "classlist-polyfill": "^1.2.0",
    "core-js": "^2.5.1",
    "html-loader": "^0.5.1",
    "intl": "^1.2.5",
