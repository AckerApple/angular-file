    "@angular/core": "4.3.6",
    "@angular/forms": "4.3.6",
    "@angular/http": "4.3.6",
    "@angular/platform-browser": "4.3.6",
    "@angular/platform-browser-dynamic": "4.3.6",
    "@types/jasmine": "^2.5.54",
    "classlist-polyfill": "1.0.3",
    "core-js": "^2.5.0",
    "html-loader": "0.4.4",
