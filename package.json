{
  "name": "angular-file",
  "version": "3.5.3",
  "private": true,
  "description": "Easy to use Angular directives for user file selections",
  "scripts": {
    "build": "npm-run-all build:prod update:dist",
    "update:dist": "node scripts/update-dist-package.js",
    "build:prod": "ng build angular-file --prod",
    "build:dev": "ng build angular-file",
    "start": "echo '\\x1b[36mTo start test env => git checkout branch gh-pages => npm start\\x1b[0m'",
    "copy:dist": "cp -R dist/. ../master",
    "deploy": "npm run save && npm run deploy:dist && npm run deploy:gh-pages",
    "deploy:dist": "npm run update:push:dist && cd ../master/ && npm publish",
    "deploy:gh-pages": "npm run update:gh-pages && npm run deploy",
    "save": "npm run build && git add . && git commit -m 'update' && git push",
    "update:gh-pages": "cd ../gh-pages && npm install angular-file",
    "update:push:dist": "npm run copy:dist && cd ../master/ && git add . && git commit -m 'update' && git push"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "Angular",
    "ng files",
    "file-upload",
    "ng-file-upload",
    "ng2-file-upload",
    "angular-file-upload"
  ],
  "author": "Acker Apple",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ackerapple/angular-file.git"
  },
