    "karma-typescript": "^3.0.8",
    "ngx-bootstrap": "^2.0.0-beta.9",
    "npm-run-all": "^4.1.2",
    "phantomjs-prebuilt": "^2.1.16",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.3",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^3.2.0",
    "ts-node": "3.3.0",
    "typescript": "2.4.2",
    "webpack": "^3.5.5",
    "zone.js": "^0.8.18",
