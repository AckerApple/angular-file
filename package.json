{
  "name": "gh-pages",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "ng test demo --browsers PhantomJS --watch=false",
    "stats": "webpack-bundle-analyzer www/stats.json",
    "stats:html": "webpack-bundle-analyzer www/stats.json --mode static --report demo/www/stats.html --no-open",
    "watch": "npm-run-all --parallel watch:demo:js watch:templates",
    "watch:demo:js": "ng serve demo --port 4201 --open",
    "build": "ng build demo --stats-json --prod --output-hashing=none",
    "compile:templates": "npm-run-all compile:templates:demo compile:template:simple-demo",
    "watch:templates": "npm-run-all --parallel \"compile:templates:demo -- --watch\" \"compile:template:simple-demo -- --watch\"",
    "compile:template:simple-demo": "ack-pug-bundler src/app/components/file-upload/simple-demo.component.ts src/app/components/file-upload/ --skipRender --oneFile --outType ts --outFileExt .template.ts",
    "compile:templates:demo": "ack-pug-bundler src/app/components src/app/components --oneToOne --includeMarkdowns --includeHtmls --outType ts --outFileExt .template.ts",
    "build:watch": "ngc -p src --watch --skip-bundles",
    "start": "npm run compile:templates && ng serve --open",
    "test:watch": "ng test",
    "copy:www": "cp -R www/. .",
    "deploy": "npm run build && npm run copy:www && git add . && git commit -m 'update' && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ackerapple/angular-file.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ackerapple/angular-file/issues"
  },
  "homepage": "https://github.com/ackerapple/angular-file#readme",
  "dependencies": {
    "angular-file": "git+https://github.com/ackerapple/angular-file.git"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/animations": "~11.0.5",
    "@angular/cli": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "^11.0.5",
    "@angular/language-service": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "14.14.16",
    "ack-pug-bundler": "^1.4.8",
    "codelyzer": "~6.0.1",
    "core-js": "^3.8.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "^11.0.3",
    "ngx-bootstrap": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "rxjs": "~6.6.3",
    "ts-node": "~9.1.1",
    "tslib": "^2.0.3",
    "tslint": "~6.1.2",
    "typescript": "4.0.5",
    "webpack-bundle-analyzer": "^4.3.0",
    "zone.js": "0.11.3"
  }
}
