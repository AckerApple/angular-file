{
  "compilerOptions": {
    "strict": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "noImplicitAny": false,
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "noEmitHelpers": false,
    "allowJs": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "../www"
  },
  "include":[
    "./index.ts"
  ]
}